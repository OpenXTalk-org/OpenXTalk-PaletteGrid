<package version="0.0">
  <name>org.openxtalk.widget.palettegrid</name>
  <title>PaletteGrid</title>
  <author>OpenXTalk</author>
  <version>1.0.0</version>
  <license>community</license>
  <type>widget</type>
  <requires name="com.livecode.library.widgetutils"/>
  <requires name="com.livecode.canvas"/>
  <requires name="com.livecode.library.iconsvg"/>
  <requires name="com.livecode.widget"/>
  <requires name="com.livecode.arithmetic"/>
  <requires name="com.livecode.binary"/>
  <requires name="com.livecode.list"/>
  <requires name="com.livecode.logic"/>
  <requires name="com.livecode.string"/>
  <requires name="com.livecode.type"/>
  <requires name="com.livecode.array"/>
  <requires name="com.livecode.byte"/>
  <requires name="com.livecode.char"/>
  <requires name="com.livecode.math"/>
  <requires name="com.livecode.engine"/>
  <requires name="com.livecode.mathfoundation"/>
  <metadata key="userVisible">true</metadata>
  <metadata key="preferredSize">200,600</metadata>
  <property name="fillRule" get="string" set="string"/>
  <metadata key="fillRule.editor">com.livecode.pi.enum</metadata>
  <metadata key="fillRule.options">non-zero,even odd</metadata>
  <metadata key="fillRule.default">non-zero</metadata>
  <metadata key="fillRule.label">Fill rule</metadata>
  <handler name="setFillRule" parameters="in string" return="nothing"/>
  <property name="foregroundColor" get="string" set="string"/>
  <metadata key="foregroundColor.editor">com.livecode.pi.colorwithalpha</metadata>
  <metadata key="foregroundColor.section">Colors</metadata>
  <metadata key="foregroundColor.label">Foreground Color</metadata>
  <handler name="setForeColor" parameters="in string" return="nothing"/>
  <handler name="getForeColor" parameters="" return="string"/>
  <property name="hiliteColor" get="string" set="string"/>
  <metadata key="hiliteColor.default">54,112,220</metadata>
  <metadata key="hiliteColor.section">Colors</metadata>
  <metadata key="hiliteColor.label">Hilite color</metadata>
  <handler name="setHiliteColor" parameters="in string" return="nothing"/>
  <handler name="getHiliteColor" parameters="" return="string"/>
  <property name="customIconSet" get="optional string" set="optional string"/>
  <metadata key="customIconSet.editor">com.livecode.pi.string</metadata>
  <metadata key="customIconSet.label">Custom IconFamily Name:</metadata>
  <metadata key="customIconSet.default">ide</metadata>
  <property name="selectedIcon" get="optional string" set="string"/>
  <property name="showFrameBorder" get="boolean" set="boolean"/>
  <property name="showNames" get="boolean" set="boolean"/>
  <property name="iconSize" get="number" set="number"/>
  <property name="filterString" get="string" set="string"/>
  <property name="showSelectedElement" get="boolean" set="boolean"/>
  <handler name="OnCreate" parameters="" return="any"/>
  <handler name="OnSave" parameters="out array" return="any"/>
  <handler name="OnLoad" parameters="in array" return="any"/>
  <handler name="OnPaint" parameters="" return="any"/>
  <handler name="OnMouseDown" parameters="" return="nothing"/>
  <handler name="OnMouseMove" parameters="" return="nothing"/>
  <handler name="onMouseLeave" parameters="" return="nothing"/>
  <handler name="OnMouseUp" parameters="" return="nothing"/>
  <handler name="OnMouseCancel" parameters="" return="nothing"/>
  <handler name="OnMouseScroll" parameters="in real,in real" return="nothing"/>
  <handler name="OnGeometryChanged" parameters="" return="any"/>
  <handler name="getPreferredSize" parameters="" return="list"/>
  <handler name="setFrameBorder" parameters="in boolean" return="any"/>
  <handler name="initialiseScrollbar" parameters="" return="any"/>
  <handler name="paintScrollbar" parameters="in Canvas,in Paint" return="any"/>
  <handler name="checkScrollbarMouseDown" parameters="" return="any"/>
  <handler name="scrollbarMouseUp" parameters="" return="any"/>
</package>